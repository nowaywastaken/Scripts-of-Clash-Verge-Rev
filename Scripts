// ==========================================
// 2. ç­›é€‰èŠ‚ç‚¹çš„é€»è¾‘ (æŒ‘é€‰é˜Ÿå‘˜)
// ==========================================

// åˆ¤æ–­æ˜¯å¦ä¸º "æ—¥å¸¸èŠ‚ç‚¹" (ç¾å›½ æˆ– æ—¥æœ¬) ä¸” (ç»ä¸è¦ é¦™æ¸¯ å’Œ å°æ¹¾)
function isDailyNode(name) {
    // å¿…é¡»åŒ…å« ç¾å›½ æˆ– æ—¥æœ¬
    const include = /(ç¾å›½|æ—¥æœ¬|US|JP|Japan|America)/i.test(name);
    // å¿…é¡» ä¸åŒ…å« é¦™æ¸¯ æˆ– å°æ¹¾
    const exclude = /(é¦™æ¸¯|å°æ¹¾|HK|TW|HongKong|Taiwan)/i.test(name);
    return include && !exclude;
}

// åˆ¤æ–­æ˜¯å¦ä¸º "æµåª’ä½“èŠ‚ç‚¹" (æ–°åŠ å¡)
function isStreamNode(name) {
    return /(æ–°åŠ å¡|SG|Singapore)/i.test(name);
}


// ==========================================
// 3. ä¸»ç¨‹åºå…¥å£
// ==========================================
function main(config) {
  // 3.1 æå–åå­—
  // ä»è®¢é˜…é‡Œæ‹¿å‡ºæ‰€æœ‰èŠ‚ç‚¹çš„åå­—
  // å¢åŠ ä¿æŠ¤ï¼Œä¸‡ä¸€æ²¡æœ‰ proxies å­—æ®µä¸æŠ¥é”™
  const proxies = config.proxies || [];
  const dailyProxies = proxies.filter(p => isDailyNode(p.name)).map(p => p.name);
  const streamProxies = proxies.filter(p => isStreamNode(p.name)).map(p => p.name);

  // 3.2 åˆ›å»ºç­–ç•¥ç»„ (ç»„å»ºé˜Ÿä¼)
  
  // é˜Ÿä¼1: æ—¥å¸¸ä¸»åŠ› (è‡ªåŠ¨æµ‹é€Ÿé€‰æœ€å¿«çš„)
  const groupDaily = {
    name: "ğŸ‡ºğŸ‡¸ğŸ‡¯ğŸ‡µ æ—¥å¸¸ä¸»åŠ›",
    type: "url-test",
    url: "http://www.gstatic.com/generate_204",
    interval: 300,
    tolerance: 50,
    proxies: dailyProxies.length > 0 ? dailyProxies : ["DIRECT"] // é˜²æ­¢æ²¡èŠ‚ç‚¹æŠ¥é”™
  };

  // é˜Ÿä¼2: æµåª’ä½“ (è‡ªåŠ¨æµ‹é€Ÿé€‰æœ€å¿«çš„)
  const groupStream = {
    name: "ğŸ‡¸ğŸ‡¬ æµåª’ä½“",
    type: "url-test",
    url: "http://www.gstatic.com/generate_204",
    interval: 300,
    tolerance: 50,
    proxies: streamProxies.length > 0 ? streamProxies : ["DIRECT"]
  };

  // æŠŠè¿™ä¸¤ä¸ªæ–°é˜Ÿä¼æ”¾åˆ°æ‰€æœ‰é˜Ÿä¼çš„æœ€å‰é¢
  if (!config["proxy-groups"]) config["proxy-groups"] = [];
  config["proxy-groups"].unshift(groupDaily, groupStream);

  // 3.3 åº”ç”¨è§„åˆ™ (è´´è·¯æ ‡)
  // æŠŠæˆ‘ä»¬çš„ myRules æ”¾åœ¨æœ€å‰é¢ï¼Œè¦†ç›–æ‰åŸæœ¬çš„è§„åˆ™
  config.rules = myRules.concat(config.rules || []);

  // 3.4 ä¼˜åŒ– DNS (é˜²æ­¢DNSæ³„éœ²æˆ–è§£ææ…¢)
  config["dns"] = {
    "enable": true,
    "ipv6": false, // å…³æ‰IPv6ä¿å¹³å®‰
    "enhanced-mode": "fake-ip",
    "fake-ip-range": "198.18.0.1/16",
    "default-nameserver": ["223.5.5.5", "119.29.29.29"],
    "nameserver": [
      "https://dns.alidns.com/dns-query", 
      "https://doh.pub/dns-query",
      "https://8.8.8.8/dns-query"
    ]
  };

  return config;
}
